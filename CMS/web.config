        </rule>
        <rule name="Bot Block Test Sites" enabled="true" stopProcessing="true">
          <match url="(.*)" />
          <conditions>
            <add input="{HTTP_HOST}" pattern="^site[0-9]{1}.pml.ac.uk" />
            <add input="{HTTP_USER_AGENT}" pattern="^(.*)Googlebot|Baidu|Bing|Yandex|Slurp(.*)" />
          </conditions>
          <action type="CustomResponse" statusCode="400" statusReason="not public" statusDescription="not public" />
        </rule>
        <rule name="PML Http to Https" enabled="true" patternSyntax="ECMAScript" stopProcessing="true">
          <match url="(.*)" />
          <conditions>
            <add input="{HTTPS}" pattern="^OFF$" />
          </conditions>
          <action type="Redirect" url="https://{HTTP_HOST}/{R:1}" redirectType="Found" />
        </rule>
      </rules>
    </rewrite>
